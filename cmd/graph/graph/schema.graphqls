type Query {
  posts: [Post!]!
}

type Post{
  ID : Int!
  Slug: String!
  Title: String!
  UserID: Int!
  Content: String!
  ImageURL: String!
  Status: String!
  Views: Int!
  CreatedAt: Int!
  UpdatedAt: Int!
}

input CreatePost{
  Slug: String!
  Title: String!
  Content: String!
  ImageUrl: String!
}


type Mutation {
  createPost(input: CreatePost!): Post!
}
